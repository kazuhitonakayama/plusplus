<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>結果</title>
</head>
<body>
    <div>結果kekka</div>
    <!-- resultsに格納されている各データをすべて表示する -->
    <% results.forEach(function(result){ %>
    <div class="result">
        <h2><%= result.user_slack_id %></h2>
        <p><%= result.points %></p>
    </div>
    <% }); %>
</body>
</html>
